<!-- //////////////////////////////////////////////////////// -->
<div id="ckviewer_container" >
    <div id="ckviewer-actionBar"></div>
    <div id="ckviewer-grid"></div>
</div>
<div id="rect" style="position:absolute;filter:Alpha(Opacity=50);-moz-opacity:0.50; opacity:0.50;background-color: #00BFFF"></div> 
<!-- //////////////////////////////////////////////////////// -->
<!-- Modal -->
<div id="ckviewerModal" class="reveal-modal small" data-reveal></div>
<script type='text/template' id='CKViewer_modalContent_templates'>
    <center><h3>{{- knowledge.title }}</h3></center><hr>
    {{= knowledge.content }}
</script>
<!-- //////////////////////////////////////////////////////// -->

<script type="text/template" id="CKViewer_action_template">
  <center>
    <input id="datepicker" type=”text” value="{{-year}}-{{-month}}-{{-day}}">
    <a href="#" id="mod1" class="button tiny secondary disabled">Mod1</a>
    <a href="#" id="mod2" class="button tiny secondary">Mod2</a>
    <a href="#" id="allconcepts" class="button tiny secondary">ResetC</a>
    <a href="#" id="allknowledges" class="button tiny secondary">ResetK</a>
    <a href="#" id="all" class="button tiny secondary">ResetAll</a>
    <a href="#" data-dropdown="drop1" class="button tiny secondary dropdown">MapLevel</a><br>
    <ul id="drop1" data-dropdown-content class="f-dropdown"></ul>
  </center>
</script>

<script type="text/template" id="CKViewer_conceptsmap_template" >
  <div id="main" >
    <ul id="port"></ul>
  </div>
</script>


<script type="text/template" id="CKViewer-Khead-template">
</script>

<script type="text/template" id="CKViewer_knowledgesmap_template">
    <div class="item">    
        <ul class="pricing-table">
            <li class="price"class="title">
                <a href="#" class="button tiny success radius expand openModal" data-category-id="{{-category.id}}">
                    <strong data-category-id="{{-category.id}}">{{- category.title}}</strong>
                </a>
            </li>
            <li class="bullet-item">
                
                {{ _.each(knowledges, function(knowledge) { }}
                    <a href="#" class="button openModal tiny radius round expand secondary CKBulle" data-knowledge-id="{{-knowledge.id}}" data-catg-origin="{{- category.title}}" style="{{-knowledge.css}}">
                        <strong data-knowledge-id="{{-knowledge.id}}" data-catg-origin="{{- category.title}}">{{- knowledge.title }}</strong>
                    </a>
                {{ }); }}   
            </li>
        </ul>
    </div>
</script>

<!-- category-notcategorized -->
<script type="text/template" id="CKViewer-notcategorized-template">
<div class="item">
    <a href="#"  class="tiny radius round openModal button secondary expand CKBulle" data-knowledge-id="{{-knowledge.id}}" style="{{-knowledge.css}}"><strong data-knowledge-id="{{-knowledge.id}}">{{- knowledge.title }}</strong></a> 
</div>
</script>



