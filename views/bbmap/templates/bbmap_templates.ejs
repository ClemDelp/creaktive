<!-- ******************************** -->
<!-- Container -->
<div id="bbmap_container" style="height:96%;position:relative;overflow:hidden;">
  <div id="top_container"></div>
  <div id="map_container" style="overflow:hidden;">
    <a href="#" id="showMenu"  class="button large cbp-openbutton alert" style="padding:0px; padding-right:10px;">
      <image  src="/img/icones/Arrowhead-Left-48.png" id="cbp-openimage"/>
      <span id="showMenu_notif"></span>
    </a>
    <nav class="cbp-spmenu cbp-spmenu-vertical cbp-spmenu-right" id="cbp-spmenu-s1">
      <div id="editor" class="panel expand">
        <ul class="tabs" data-tab>
          <li class="tab-title active"><a href="#editModel"><img title="edit" src="/img/icones/Edit.png"/><span id="notifDesc"></span></a></li>
          <li class="tab-title"><a href="#css3Model"><img title="template" src="/img/icones/Shape.png"/></a></li>
          <li class="tab-title"><a href="#activitiesModel"><img title="template" src="/img/icones/Bell-24-circle.png"/></a></li>
          <li class="tab-title"><a href="#googleSearchModel"><img title="web data" src="/img/icones/Web-24.png"/></a></li>
        </ul>
        <div class="tabs-content">
          <div id="editModel" class="content active">
            <span id="editorPart"></span>
            <span id="attachmentPart"></span>
            <span id="commentPart"></span>
          </div>
          <div id="css3Model" class="content"></div>
          <div id="activitiesModel" class="content"></div>
          <div id="googleSearchModel" class="content"></div>
        </div>
    </div>
    </nav>
    <div id="map" class="demo chart-demo"></div>
  </div>
  
  <div id="bottom_container"></div>
</div>
<!-- ******************************** -->
<script type="text/template" id="bbmap-tooltip-notif-template">
  <span class="tooltip_notif" onClick="bbmap.views.main.openTooltipNotif({{- notif.to.id }})" >
    {{- notif.content }} by 
    <img style="float:right" width="15" title="last update by {{-notif.from.name }}" src="{{- notif.from.img }}"/>
    <b>{{- notif.from.name }}</b>
  </span>
</script>

<script type="text/template" id="bbmap-joyride-template">
  <ol class="joyride-list" style="z-index:9999999" data-joyride>
    <li id="joyride_id" data-id="joyride" data-text="Ok" data-options="tip_location: top">
      <br>
      <input id="joyride_val" class="tiny round" type="text" placeholder="name..."><lable><small>(Press ENTER)</small></label>
    </li>
  </ol>
</script>

    
<!-- <span id="{{-model.id}}" data-type="action" class="button tiny round secondary replier icon">-</span> -->
<script type="text/template" id="bbmap-bulle-template">
    <span data-type="title" style="float:right" id="{{-model.id}}_tooltip"></span>
    <img id="{{-model.id}}" data-type="title" style="float:left" width="15" title="create by {{-user.name }}" src="{{-user.img }}"/>
    
    <span id="{{-model.id}}_joyride"></span>
    <span id="{{-model.id}}" data-type="title" class="editTitle">{{-model.title}}</span>
    <span id="{{-model.id}}" data-type="action" class="label round secondary ep icon">+c</span>
    <span id="{{-model.id}}" data-type="action" class="label round secondary ep2 icon">+k</span>
    <span id="{{-model.id}}" data-type="action" class="label round secondary ep3 icon"><img data-type="action" id="{{-model.id}}" title="mode" src="/img/icones/Edit.png"/></span>
    <span id="{{-model.id}}" data-type="action" class="label round secondary sup icon"><img data-type="action" id="{{-model.id}}" title="mode" src="/img/icones/Delete.png"/></span>
    {{ if( (model.comments.length + model.attachment.length) >0) { }}
    <span id="{{-model.id}}" data-type="title" class="info top-bar-unread">{{- model.comments.length + model.attachment.length}}</span>
    {{ } }}

</script>

<script type="text/template" id="bbmap-top-element-template">
  <div class="large-6 medium-6 small-6 columns">
    <button class="button simplePadding secondary radius" data-dropdown="title_project_dropdown" aria-controls="title_project_dropdown" aria-expanded="true">
        <b>
        {{-project.title}}
        {{ if(project.starred){ }}<img data-dropdown="title_project_dropdown"  title="starred project" src="/img/icones/Shape-Star2-32.png" />{{ } }}
        {{ if(project.status == 'public'){ }}
          <img data-model-id="{{-project.id}}" data-dropdown="title_project_dropdown"  title="public project" src="/img/icones/Open-32.png"/>
        {{ }else{ }}
          <img data-model-id="{{-project.id}}" data-dropdown="title_project_dropdown"  title="private project" src="/img/icones/Lock-32.png"/>
        {{ } }}
        </b>
    </button>
    <div id="title_project_dropdown" class="f-dropdown content medium" data-dropdown-content="" aria-hidden="false" tabindex="-1" ></div>

    <button class="button secondary simplePadding radius" data-dropdown="members_anager_dropdown" aria-controls="members_anager_dropdown" aria-expanded="true"><b>Members</b><img data-type="action" title="mode" src="/img/icones/Group-Add-32.png"/></button>
    <div id="members_anager_dropdown" class="f-dropdown content medium" data-dropdown-content="" aria-hidden="false" tabindex="-1" ></div>

    <a style="display:true" href="/timela?projectId={{-project.id}}" class="button secondary tiny radius timelinePresentation"><img src="/img/icones/Bullets-32-blue.png" /></a>

  </div>
  <div class="large-3 medium-3 small-3 columns">
    {{ if(mode == "visu"){ }}
        <a href="#edit" class="button simplePadding alert radius"><b>Edit</b><img data-type="action" title="mode" src="/img/icones/Pencil-32-white.png"/></a>
      {{ }else if(mode == "edit"){ }}
        {{ if((filter == "c")||(filter == "ck")||(filter == "ckp")){ }}
            <a style="padding:10px" href="#" id="dropC" class="drop dropC button tiny success round" title="Drag & Drop Concept to Map">
              <img title="Drag & Drop Concept to Map" width="24px" class="drop" src="/img/icones/add-concept-32-white.png"/>
            </a>
        {{ } }}
        {{ if((filter == "p")||(filter == "kp")||(filter == "ckp")){ }}
          <a style="padding:10px" href="#" id="dropP" class="drop dropP button tiny pocheDesign round" title="Drag & Drop Category to Map">
            <img title="Drag & Drop Category to Map" width="24px" class="drop" src="/img/icones/add-poche-32-white.png"/>
          </a>
        {{ } }}
        {{ if((filter == "k")||(filter == "ck")||(filter == "ckp")||(filter == "kp")){ }}
          <a style="padding:10px" href="#" id="dropK" class="drop dropK button tiny round" title="Drag & Drop Knowledge to Map">
            <img title="Drag & Drop Knowledge to Map" width="24px" class="drop" src="/img/icones/add-k-32-white.png"/>
          </a>
        {{ } }}
      {{ } }}
  </div>

  <div class="large-3 medium-3 small-3 columns">
    <div style="float:right">
      <a href="#" class="tiny round button secondary zoomin"><img title="zoom-out" src="/img/icones/Zoom-Out-24.png"/></a>
      <a href="#" class="tiny round button secondary zoomout"><img title="zoom-in" src="/img/icones/Zoom-In-24.png"/></a>
      <a href="#" class="tiny round button secondary reset"><img title="reset" src="/img/icones/Fit-To-Size-24.png"/></a>
      <a href="#" class="tiny round button secondary fullscreen"><img title="full-screen" src="/img/icones/Full-Screen-Expand-24.png"/></a>
      <small style="display:none" id="zoom_val"></small>
    </div>
  </div>

</script>

<script type="text/template" id="bbmap-bottom-element-template">
  <span id="bbmap_bottom_ui">
    <center>
      {{ if(mode != "timeline"){ }}
        <span style="display:none">
          <img title="zoom-in" src="/img/icones/Light-32.png"/>
          <select id="visu_select_mode" class="large-1 medium-1 small-1">
            <option value="children">Display children & linked knowledge</option>
            <option value="parent">Display parents & linked knowledge</option>
            <option value="node">Display concept & linked knowledge</option>
          </select>
        </span>

        
        <img title="zoom-in" src="/img/icones/Filtering-24.png"/> 
        <select id="filterSelection" class="large-1 medium-1 small-1">
          <option value="c" {{ if(filter == "c"){ }}selected{{ } }}>Concepts</option>
          <option value="kp" {{ if(filter == "kp"){ }}selected{{ } }}>Knowledges & Categories</option>
          <option value="ckp" {{ if(filter == "ckp"){ }}selected{{ } }}>All</option>
        </select>

      {{ } }}
      {{ if((filter == "ck")||(filter == "ckp")){ }}
        <a style="display:none" href="#" class="tiny round button secondary ckOperator"><img title="display CK links" src="/img/icones/Data-Share-24.png"/></a>
      {{ } }}
      <a href="#" class="button tiny round secondary downloadimage"><img title="Download screenshot" src="/img/icones/Download-24.png"/></a>
      {{ if(mode == "edit"){ }} 
        <a href="#" data-reveal-id="importData_container" class="button tiny round secondary"><img title="Import data from an other project" src="/img/icones/Data-Import-24.png"/></a>
        <div id="importData_container" class="reveal-modal large" data-reveal></div>
        <a href="#" class="tiny round button secondary structureSubTree"><img title="structure sub-tree" src="/img/icones/Network-Drives-24.png"/></a>
        <a style="display:none" href="#" class="button tiny round secondary prevH"><img title="ctrl-Z" src="/img/icones/Arrowhead-Left-24.png"/></a>
        <a style="display:none" href="#" class="button tiny round secondary nextH"><img title="ctrl-Y" src="/img/icones/Arrowhead-Right-24.png"/></a>
      {{ } }}
    </center>
  </span>
</script>


