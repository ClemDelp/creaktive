<!-- ******************************** -->
<!-- Container -->
<div id="bbmap_container" style="height:96%;position:relative;overflow:hidden;">
  <div id="top_container"></div>
  <div id="map_container" style="overflow:hidden;">
    
    <span id="stat_left" style="left:0px;position:fixed;top:30%;z-index:10000000;"></span>

    <span id="right_buttons" style="right:0px;position:fixed;top:50%;width:70px;z-index:10000000;display:none">
      <a href="#" style="float:right" class="borderBlue pulseBlue tiny radius button secondary edit"><img title="edit" src="/img/icones/Edit.png"/></a>
      <a href="#" style="float:right;display:none" class="borderBlue tiny radius button secondary design"><img title="template" src="/img/icones/Shape.png"/></a>
      <a href="#" style="float:right;display:none" class="borderBlue tiny radius button secondary history"><img title="template" src="/img/icones/Bell-24-circle.png"/></a>
    </span>
    
    <nav id="sideBar">
      <a href="#" class="button radius tiny alert closeSibeBar" style="position:absolute;right:0px;z-index:1000000">X</a>
      <div id="sideBar_container" style="height:100%;overflow:auto"></div>
    </nav>
    
    <div id="map" class="demo chart-demo"></div>
  
  </div>
  
</div>
<!-- ******************************** -->

<script type="text/template" id="bbmap-tooltip-notif-template">
  <span class="tooltip_notif" onClick="bbmap.views.main.openTooltipNotif({{- notif.to.id }})" >
    {{- notif.content }} by 
    <img style="float:right" width="15" title="last update by {{-user.name }}" src="{{- user.img }}"/>
    <b>{{- user.name }}</b>
  </span>
</script>

<script type="text/template" id="bbmap-joyride-template">
  <ol class="joyride-list" style="z-index:9999999" data-joyride>
    <li id="joyride_id" data-id="joyride" data-text="Ok" data-options="tip_location: top">
      <br>
      <input id="joyride_val" class="tiny round" type="text" placeholder="name..."><lable><small>(Press ENTER)</small></label>
    </li>
  </ol>
</script>

    
<!-- <span id="{{-model.id}}" data-type="action" class="button tiny round secondary replier icon">-</span> -->
<script type="text/template" id="bbmap-bulle-template">
  <span data-type="title" style="float:right" id="{{-model.id}}_tooltip"></span>
  <img id="{{-model.id}}" data-type="title" style="float:left" width="15" title="create by {{-user.name }}" src="{{-user.img }}"/>
  
  <span id="{{-model.id}}_joyride"></span>
  <span id="{{-model.id}}" data-type="title" class="editTitle">{{-model.title}}</span>
  <span id="{{-model.id}}" data-type="action" class="label round secondary ep icon">+c</span>
  <span id="{{-model.id}}" data-type="action" class="label round secondary ep2 icon">+k</span>
  <span id="{{-model.id}}" data-type="action" class="label round secondary ep3 icon"><img data-type="action" id="{{-model.id}}" title="mode" src="/img/icones/Edit.png"/></span>
  
  <span id="{{-model.id}}" data-type="action" class="label round secondary sup icon">
    <img data-type="action" id="{{-model.id}}" title="mode" src="/img/icones/Delete.png"/>
  </span>

  
  {{ if(model.visibility == true){ }}
  <span id="{{-model.id}}" data-type="action" style="background:transparent;padding:0" class="label secondary round expcoll tiny icon">
    <img data-type="action" id="{{-model.id}}" title="mode" src="/img/icones/Expand-Collapse-32.png"/>
  </span>
  {{ }else if(model.visibility == false){ }}
    <span id="{{-model.id}}" data-type="action" style="background:transparent;" class="label secondary round expcoll tiny">
      <small style="margin-bottom:0px;padding:0 5px 0 5px;color:red" data-type="action" id="{{-model.id}}">
        <b data-type="action" id="{{-model.id}}">{{-childs_nbr}}</b>
      </small>
    </span>
  {{ } }}

</script>


