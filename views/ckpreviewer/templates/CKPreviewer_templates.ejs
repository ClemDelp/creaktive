<!-- //////////////////////////////////////////////////////// -->
<div id="CKPreviewer_container" ></div>
<div id="rect" style="position:absolute;filter:Alpha(Opacity=50);-moz-opacity:0.50; opacity:0.50;background-color: #00BFFF"></div> 
<div class="zoomViewport demo" style="position:absolute; left:0%; top:0%; width:100%; hight:100%">
    <div class="zoomContainer"style=" left:0%; top:0%; width:100%; hight:100%">
    </div>
</div>
<!-- //////////////////////////////////////////////////////// -->
<!-- Modal -->
<div id="CKPreviewerModal" class="reveal-modal" data-reveal></div>
<script type='text/template' id='CKPreviewer_modalContent_templates'>
    <center><h3>{{- knowledge.title }}</h3></center><hr>
    {{= knowledge.content }}
</script>
<!-- //////////////////////////////////////////////////////// -->
<script type="text/template" id="CKPreviewer_action_template" >
  <a href="#" id="select" class="button tiny expad ">Zoom</a>
  <a href="#" id="slide" class="button tiny expad ">Add</a>
</script>

<script type="text/template" id="CKPreviewer_conceptsmap_template" >
  <center><h3>C</h3></center><hr>
  <div id="main" >
    <ul id="port" ></ul>
  </div>
</script>

<script type="text/template" id="CKPreviewer_knowledgesmap_template">
    <div class="item">    
        <ul class="pricing-table">
            <li class="price"class="title">
                <a href="#" class="button tiny success radius expand openModal" data-category-id="{{-category.id}}">
                    <strong data-category-id="{{-category.id}}">{{- category.title}}</strong>
                </a>
            </li>
            <li class="bullet-item">
                
                {{ _.each(knowledges, function(knowledge) { }}
                    <a href="#" class="button openModal tiny radius round expand secondary CKBulle" data-knowledge-id="{{-knowledge.id}}" data-catg-origin="{{- category.title}}"  style="border-color:{{-knowledge.color}};color:{{-knowledge.color}}">
                        <strong data-knowledge-id="{{-knowledge.id}}" data-catg-origin="{{- category.title}}">{{- knowledge.title }}</strong>
                    </a>
                {{ }); }}   
            </li>
        </ul>
    </div>
</script>

<!-- category-notcategorized -->
<script type="text/template" id="CKPreviewer-notcategorized-template">
<div class="item">
    <a href="#"  class="tiny radius round openModal button expand secondary CKBulle" data-knowledge-id="{{-knowledge.id}}" style="border-color:{{-knowledge.color}};color:{{-knowledge.color}}"><strong data-knowledge-id="{{-knowledge.id}}">{{- knowledge.title }}</strong></a> 
</div>
</script>

<script type="text/template" id="CKPreviewer-Khead-template">
  <a href="#" class="button tiny secondary fullScreen" style="float:right">
    <img style="" class="large-6 medium-6 small-6" src="img/fullscreen.png" title="fullscreen/minimize">
  </a>
  <a href="#" id="allknowledges" class="button tiny secondary " style="float:left">
    reset
  </a>
  <center><h3>K</h3></center><hr>
</script>


<script type="text/template" id="CKPreviewer_slides_template" >
<div class="zoomButton" data-type="prev" data-root=".demo">prev</div>
<div class="zoomButton" data-type="next" data-root=".demo">next</div>
  <ul class = "sortable">
    {{ _.each(slides, function(slide) { }}
        <li id = "slide" style = "list-style: none;border: 1px solid #CCC;background: #F6F6F6;color: #1C94C4;margin: 5px;padding: 5px;line-height: 80px;float: left;width: 80px;height: 80px;text-align: center;" slideorder="{{-slide.slideorder}}">
        slide{{-slide.slideorder+1}}
        </li>
    {{ }); }}
  </ul>
</script>
