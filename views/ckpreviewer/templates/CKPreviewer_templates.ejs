<!-- //////////////////////////////////////////////////////// -->
<div id="CKPreviewer_container" ></div>
<!-- //////////////////////////////////////////////////////// -->
<!-- Modal -->
<div id="CKPreviewerModal" class="reveal-modal" data-reveal></div> 
<script type='text/template' id='CKPreviewer_modalContent_templates'>
    <a href="#" id="selectzoon" class="button tiny  " style="float:left">Select</a>
    <a href="#" id="newimage" class="button tiny  " style="float:left">New</a>
    <a href="#" id="resizeimage" class="button tiny  " style="float:left">Resize</a>
    <center><h3></h3></center><hr>
    <image src="/S3/getPrivateUrl?amz_id={{- src}}"  id="cropbox"/><hr>
    <form >
      <button type="submit" id="addimage" style="float:right">Add</button>
      <label>Choose Image Size</label>
      <input type="radio" name="imagesize" value="Small"  id="smallimage" checked=true><label for="smallimage">Small</label>
      <input type="radio" name="imagesize" value="Medium" id="mediumimage"><label for="mediumimage">Medium</label>
      <input type="radio" name="imagesize" value="Large" id="largeimage"><label for="largeimage">Large</label>
    </form>
    <a class="close-reveal-modal">&#215;</a>
</script>

<!-- //////////////////////////////////////////////////////// -->
<!-- Action -->
<script type="text/template" id="CKPreviewer_action_template" >
  <div class="expand">
    <div class="small-6 large-6 medium-6 columns">
      <center>
        
      <a href="#" id="drop0" data-dropdown="drop1" class="button tiny secondary dropdown"  style="float:right">Presentation</a><br>
      <ul id="drop1" data-dropdown-content class="f-dropdown">
      {{ _.each(presentations, function(presentation) { }}
        <li>
          <a id="presentation" href="#" presentation_id="{{- presentation.id}}">{{- presentation.title}} </a>
        </li>
      {{ }); }}  
      </ul>
      </center>
    </div>
    <div class="small-3 large-3 medium-3 columns">
      <div class="row collapse custom_row">
        <div class="small-8 columns">
          <input class="presentation_title" type="text" placeholder="new presentation..."/>
        </div>
        <div class="small-2 columns">
          <a href="#" id="add1" class="button success postfix">+</a>         
        </div>
        <div class="small-2 columns">          
          <a href="#" id="delete"  class="button postfix"  >-</a>
        </div>
      </div>
    </div>
    <div class="small-3 large-3 medium-3 columns">
      <a href="#" id="slideshow"  class="button  secondary tiny"  >Slideshow</a>
    <br>
    </div>
  </div>
  <div class="row"></div>
</script>


<!-- //////////////////////////////////////////////////////// -->
<!-- Left part -->
<script type="text/template" id="CKPreviewer_images_template" >
  <ul class="radius tabs tiny" data-tab>
    <li class="tab-title active"><a href="#panel2-1">I</a></li>
    <li class="tab-title"><a href="#panel2-2">K</a></li>
    <li class="tab-title"><a href="#panel2-3">C</a></li>
    <li class="tab-title"><a href="#panel2-4">P</a></li>
  </ul>
  <div class="tabs-content">
    <div class="content active" id="panel2-1">
    <ul class="small-block-grid-2">
      {{ _.each(images, function(image) { }}
        <li id="image_container">
          {{- image.date}}
          <a id="deleteimage" data-image-id="{{- image.src}}" data-screenshot-id="{{- image.id}}" style="float:right">&#215;</a>
          <img class="th" id="image" data-image-id="{{- image.src}}" data-screenshot-id="{{- image.id}}" src="/S3/getPrivateUrl?amz_id={{- image.src}}" style="width: 300px;height: 150px">
        </li>
      {{ }); }}  
    </ul>
    </div>
    <div class="content" id="panel2-2">
      <div class="row ">
        <div class="small-4 large-4 medium-4 columns">
          <a href="#" id="ImportAllknowledges"  class="button  secondary tiny"  style="float:left">Import all</a>
        </div>
        <div class="small-8 large-8 medium-8 columns">
            <div class="row collapse custom_row">
                <div class="small-10 columns">
                  <input id="knowledge_title" type="text" class="searchK" placeholder="find your knowledge..."/>
                </div>
                <div class="small-2 columns">
                  <a href="#" class="button postfix ">Search</a>
                </div>
            </div>
        </div>
      </div>
      <div id="ks"></div>
    </div>

    <div class="content" id="panel2-3">
      <div class="row ">
        <div class="small-4 large-4 medium-4 columns">
          <a href="#" id="ImportAllconcepts"  class="button  secondary tiny"  style="float:left">Import all</a>
        </div>
        <div class="small-8 large-8 medium-8 columns">
            <div class="row collapse custom_row">
                <div class="small-10 columns">
                  <input id="concept_title" type="text" class="searchC" placeholder="find your concept..."/>
                </div>
                <div class="small-2 columns">
                  <a href="#" class="button postfix ">Search</a>
                </div>
            </div>
        </div>
      </div>
      <div id="cs"></div>
    </div>

    <div class="content" id="panel2-4">
      <div class="row ">
        <div class="small-4 large-4 medium-4 columns">
          <a href="#" id="ImportAllpoches"  class="button  secondary tiny"  style="float:left">Import all</a>
        </div>
        <div class="small-8 large-8 medium-8 columns">
            <div class="row collapse custom_row">
                <div class="small-10 columns">
                  <input id="poche_title" type="text" class="searchP" placeholder="find your category..."/>
                </div>
                <div class="small-2 columns">
                  <a href="#" class="button postfix ">Search</a>
                </div>
            </div>
        </div>
      </div>
      <div>
    <div id="ps"></div>
  </div>
</script>
<!-- //////////////////////////////////////////////////////// -->
<script type="text/template" id="CKPreviewer_ks_template" >
  {{ _.each(knowledges, function(knowledge) { }}
    <a href="#" class="button knowledge tiny" style="{{-knowledge.css}}" data-knowledge-id="{{-knowledge.id}}" data-knowledge-content="{{-knowledge.content}}" data-knowledge-title="{{-knowledge.title}}" style="border-color:{{-knowledge.color}};color:{{-knowledge.color}}">
      <strong data-knowledge-id="{{-knowledge.id}}" data-knowledge-content="{{-knowledge.content}}" data-knowledge-title="{{-knowledge.title}}">{{- knowledge.title }}</strong>
    </a>
  {{ }); }}  
</script>
<!-- //////////////////////////////////////////////////////// -->
<script type="text/template" id="CKPreviewer_cs_template" >
  {{ _.each(concepts, function(concepte) { }}
    <a href="#" class="button concept tiny" style="{{-concepte.css}}" data-concepte-id="{{-concepte.id}}" data-concepte-content="{{-concepte.content}}" data-concepte-title="{{-concepte.title}}">
      <strong data-concepte-id="{{-concepte.id}}" data-concepte-content="{{-concepte.content}}" data-concepte-title="{{-concepte.title}}">{{- concepte.title }}</strong>
    </a>
  {{ }); }} 
</script>
<!-- //////////////////////////////////////////////////////// -->
<script type="text/template" id="CKPreviewer_ps_template" >
  {{ _.each(poches, function(poche) { }}
    <a href="#" class="button poche tiny" style="{{-poche.css}}" data-poche-id="{{-poche.id}}" data-poche-cotent="{{-poche.content}}" data-poche-title="{{-poche.title}}">
      <strong data-poche-id="{{-poche.id}}" >{{- poche.title }}</strong>
    </a>
  {{ }); }} 
</script>

<!-- //////////////////////////////////////////////////////// -->
<!-- Right part -->
<script type="text/template" id="CKPreviewer_editer_template" >
    <div id="textzoon">
        <textarea  class="ckeditor"  name="ckeditor" ></textarea> <hr>
        <a href="#" class="button tiny round  export"style="float:right">Export</a>
        <a href="#" class="button tiny round  createpresentation"style="float:right">Update</a>
        <a href="#" class="button tiny round  clearpresentation"style="float:right">Clear</a>   
        <a href="#" class="button tiny round  resetpresentation"style="float:right">Reset</a>  
    </div>
</script>

