<!-- //////////////////////////////////////////////////////// -->
<div id="CKPreviewer_container" >
  <br>
  <a href="#" id="showMenu"  class="button tiny secondary radius cbp-openbutton">
    <image  src="/img/icones/Arrow-Left.png"width="32px"  id="cbp-openimage"/>
  </a>
  <nav class="cbp-spmenu cbp-spmenu-vertical cbp-spmenu-right" id="cbp-spmenu-s1" style="overflow: auto;">
    <div id="leftpart" style="padding:15px"></div>
  </nav>
  <!-- main content goes here -->
  <div id="mainpart"></div>
</div>
<!-- //////////////////////////////////////////////////////// -->
<!-- Modal -->
<div id="CKPreviewerModal" class="reveal-modal" data-reveal style="z-index: 2000;"></div> 
<script type='text/template' id='CKPreviewer_modalContent_templates'>
    <a href="#" id="resizeimage" class="button tiny  " style="float:left">ResizeImage</a>
    <a href="#" id="newimage" class="button tiny  success" style="float:left">CreateNewImage</a>
    <image origin="*" src={{- src}}  id="cropbox"/><hr>
    <a href="#" id="confirm" class="button tiny  " style="float:right">OK</a>
    <a class="close-reveal-modal">&#215;</a>
</script>

<!-- //////////////////////////////////////////////////////// -->
<!-- Main part -->
<script type="text/template" id="CKPreviewer_editer_template" >
  <div class="row panel">
      <div class="row">
        <div class="small-8 medium-8 large-8 columns">
          <a href="#" class="return" style="">
            <image  src="/img/icones/Arrow-Left.png"width="32px"  id="cbp-openimage"/>
            Back
          </a>
          <h2>{{-cp.title}}</h2>
        </div>
        <div class="small-4 medium-4 large-4 columns">
          <a href="#" class="button tiny secondary round export"><img title="export" src="/img/icones/Export-24.png"/></a>
          <a href="#" class="button tiny secondary round clearpresentation"><img title="clear" src="/img/icones/Clean-24.png"/></a>
          <a href="#" class="button tiny secondary round delete"><img title="delete" src="/img/icones/Delete.png"/></a>
        </div>
      </div>
      <hr>
      <div id="textzoon" class="row">
          <textarea  class="ckeditor"  name="ckeditor" ></textarea> 
      </div>
  </div>
</script>
<!-- Action -->
<script type="text/template" id="CKPreviewer_action_template" >
  <center><h2>Reports</h2></center>
  <hr>
  <div  id="categories_grid">
    
    <div style="padding:10px"><div class="row collapse custom_row"></div>
      <div class="small-10 columns">
        <input class="presentation_title" type="text" placeholder="New presentation"/>
      </div>
      <div class="small-2 columns">
        <a href="#" id="add1" class="button postfix ">+</a>
      </div>
    </div>
    {{ _.each(presentations, function(presentation) { }}
      <div class="item">    
        <ul class="pricing-table">
          <li class="title" style="background:#F2F2F2;color:#008CBA">{{- presentation.title}}</li>
          <li class="description" id="content{{- presentation.id}}"></li>
          <li class="bullet-item">{{- presentation.updatedAt}}</li>
          <li class="bullet-item">{{- presentation.user_name}}</li>
          <li class="cta-button"><a presentation_id="{{- presentation.id}}" class="button tiny radius expand presentation" href="#">Open</a></li>
        </ul>
      </div>
    {{ }); }}  
</div>
</script>

<script type="text/template" id="CKPreviewer_image_template" >


</script>


<!-- //////////////////////////////////////////////////////// -->
<!-- Left part -->
<script type="text/template" id="CKPreviewer_images_template" >
  <ul class="tabs" data-tab>
    <li class="tab-title active"><a href="#panel2-3"><img title="concepts" src="/img/icones/Light-Bulb-24.png"/></a></li>
    <li class="tab-title"><a href="#panel2-2"><img title="knowledges" src="/img/icones/Book-Open-24.png"/></a></li>
    <li class="tab-title"><a href="#panel2-4"><img title="categories" src="/img/icones/Category-24.png"/></a></li>
    <li class="tab-title"><a href="#panel2-1"><img title="screenshots" src="/img/icones/Images-24.png"/></a></li>
  </ul>
  <div class="tabs-content">
    <div class="content " id="panel2-1">
    <ul class="small-block-grid-2" id="imageList">
      {{ _.each(images, function(image,n) { }}
        <li id="image_container" class="imgbox">
          <span class="label radius  secondary">{{- image.date}}</span>
          <span class="label radius  downloadimage" data-image-id="{{- image.src}}" data-image-date="{{- image.date}}">download</span>
          <span class="label radius alert editimage" data-image-id="{{- image.src}}" data-screenshot-id="{{- image.id}}">edit</span>
          <a id="deleteimage" data-image-id="{{- image.src}}" data-screenshot-id="{{- image.id}}" style="float:right">&#215;</a>
          <img class="th dropdown" crossorigin="*" id="image"  src='/s3/getUrl?amz_id={{- image.src}}' data-image-id="{{- image.src}}" >
        </li>
      {{ }); }}  
    </ul>
    </div>
    <div class="content" id="panel2-2">
      <div class="row ">
        <div class="small-4 large-4 medium-4 columns">
          <a href="#" id="ImportAllknowledges"  class="button  secondary tiny" style="float:left">Import all</a>
        </div>
        <div class="small-8 large-8 medium-8 columns">
            <div class="row collapse custom_row">
                <div class="small-10 columns">
                  <input id="knowledge_title" type="text" class="searchK" placeholder="find knowledge"/>
                </div>
                <div class="small-2 columns">
                  <a href="#" class="button postfix secondary"><img src="/img/search-logo.svg"width="16px"></a>
                </div>
            </div>
        </div>
      </div>
      <div id="ks"></div>
    </div>

    <div class="content active" id="panel2-3">
      <div class="row ">
        <div class="small-4 large-4 medium-4 columns">
          <a href="#" id="ImportAllconcepts"  class="button  secondary tiny"  style="float:left">Import all</a>
        </div>
        <div class="small-8 large-8 medium-8 columns">
            <div class="row collapse custom_row">
                <div class="small-10 columns">
                  <input id="concept_title" type="text" class="searchC" placeholder="find concept"/>
                </div>
                <div class="small-2 columns">
                  <a href="#" class="button postfix secondary"><img src="/img/search-logo.svg"width="16px"></a>
                </div>
            </div>
        </div>
      </div>
      <div id="cs"></div>
    </div>

    <div class="content" id="panel2-4">
      <div class="row ">
        <div class="small-4 large-4 medium-4 columns">
          <a href="#" id="ImportAllpoches"  class="button  secondary tiny"  style="float:left">Import all</a>
        </div>
        <div class="small-8 large-8 medium-8 columns">
            <div class="row collapse custom_row">
                <div class="small-10 columns">
                  <input id="poche_title" type="text" class="searchP" placeholder="find category"/>
                </div>
                <div class="small-2 columns">
                  <a href="#" class="button postfix secondary"><img src="/img/search-logo.svg"width="16px"></a>
                </div>
            </div>
        </div>
      </div>
      <div>
    <div id="ps"></div>
  </div>
</script>
<!-- //////////////////////////////////////////////////////// -->
<script type="text/template" id="CKPreviewer_ks_template" >
  {{ _.each(knowledges, function(knowledge) { }}
    <a href="#" class="button knowledge tiny" style="{{-knowledge.css}}" data-knowledge-id="{{-knowledge.id}}" data-knowledge-content="{{-knowledge.content}}" data-knowledge-title="{{-knowledge.title}}" style="border-color:{{-knowledge.color}};color:{{-knowledge.color}}">
      <strong data-knowledge-id="{{-knowledge.id}}" data-knowledge-content="{{-knowledge.content}}" data-knowledge-title="{{-knowledge.title}}">{{- knowledge.title }}</strong>
    </a>
  {{ }); }}  
</script>
<!-- //////////////////////////////////////////////////////// -->
<script type="text/template" id="CKPreviewer_cs_template" >
  {{ _.each(concepts, function(concepte) { }}
    <a href="#" class="button concept tiny" style="{{-concepte.css}}" data-concepte-id="{{-concepte.id}}" data-concepte-content="{{-concepte.content}}" data-concepte-title="{{-concepte.title}}">
      <strong data-concepte-id="{{-concepte.id}}" data-concepte-content="{{-concepte.content}}" data-concepte-title="{{-concepte.title}}">{{- concepte.title }}</strong>
    </a>
  {{ }); }} 
</script>
<!-- //////////////////////////////////////////////////////// -->
<script type="text/template" id="CKPreviewer_ps_template" >
  {{ _.each(poches, function(poche) { }}
    <a href="#" class="button poche tiny" style="{{-poche.css}}" data-poche-id="{{-poche.id}}" data-poche-cotent="{{-poche.content}}" data-poche-title="{{-poche.title}}">
      <strong data-poche-id="{{-poche.id}}" >{{- poche.title }}</strong>
    </a>
  {{ }); }} 
</script>

<!-- //////////////////////////////////////////////////////// -->


