<script type="text/template" id="imagesList-diapo-template">

	<fieldset>
        <legend align=left>Images ({{-images.length}})</legend>
        <ul id="imageList"  class="clearing-thumbs" data-clearing >
        {{ _.each(images, function(image){ }}
        	<li class="large-3 medium-3 small-3">
        		<a href="/s3/getUrl?amz_id={{- image.url }}"><img crossorigin="*" class="large-12 medium-12 small-12 th" title="{{- image.name }}" src="/s3/getUrl?amz_id={{- image.url }}"></a>
        	</li>
    	{{ }) }}

		</ul>
    </fieldset>

</script>



