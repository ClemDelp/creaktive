<!-- Modules to load -->
<%- partial('templates/editProfile_templates') %>
<script src="js/BB_app/modules/editProfile.js?v=<%= sails.config.session.version %>"></script>


<script>
  $(document).ready(function () {

    Backbone.Model.prototype.toJSON = function() {
      return JSON.parse(JSON.stringify(this.attributes));
    };

    rt(io, function(){
      var json = {};
      json.users = <%- users %>;
      json.user = <%- currentUser %>;
      json.projects = <%- projects %>;
      json.activityLog = <%- activityLog %>;
      json.permissions = <%- permissions %>;


      global.init(json, function(){
        // Modules
        topbar.init({el:"#topbar_container"});
        editProfile.init();     
        /*activat of "hashchange events's monitoring"*/
        if(!Backbone.History.started) Backbone.history.start();  
        
      });
      $(document).foundation();
    });
  }) ;
</script>





<script>
    function select (data) {
        $('#image').val(data);
        $('#avatar').attr('src', data);
    }
</script>

